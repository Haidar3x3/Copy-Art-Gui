local oldNew = Instance.new
Instance.new = function(className, parent)
    local newObj = oldNew(className, parent)
    
    -- Block ScreenGuis or other GUI elements
    if className == "ScreenGui" or className == "Frame" or className == "TextButton" then
        newObj:Destroy() -- Immediately destroy it
        return newObj -- Return the destroyed object (won't show)
    end
    
    return newObj
end

print("GUI blocking active! ScreenGuis, Frames, and Buttons will be destroyed.")
